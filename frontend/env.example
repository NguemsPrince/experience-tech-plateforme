# Configuration de l'application
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_APP_NAME=Expérience Tech
REACT_APP_VERSION=1.0.0

# Stripe Paiement
# Obtenez vos clés sur https://stripe.com
# 1. Créez un compte Stripe gratuit
# 2. Allez dans Developers > API keys
# 3. Copiez votre clé publique (publishable key)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Exemple clé de test :
# REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_51AbCdEfGhIjKlMnOpQrStUvWxYz1234567890

# Google Analytics
REACT_APP_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Mobile Money APIs
REACT_APP_MOMO_API_URL=https://api.momo.td
REACT_APP_MOMO_API_KEY=your_momo_api_key
REACT_APP_MOMO_MERCHANT_ID=your_momo_merchant_id

REACT_APP_AIRTEL_API_URL=https://api.airtel.td
REACT_APP_AIRTEL_API_KEY=your_airtel_api_key
REACT_APP_AIRTEL_MERCHANT_ID=your_airtel_merchant_id

# Monitoring
REACT_APP_SENTRY_DSN=your_sentry_dsn
REACT_APP_MONITORING_ENABLED=true

# Features
REACT_APP_AI_CHATBOT_ENABLED=true
REACT_APP_ANALYTICS_ENABLED=true
REACT_APP_MONITORING_ENABLED=true
REACT_APP_CRM_ENABLED=true

# Performance
REACT_APP_LAZY_LOADING=true
REACT_APP_IMAGE_OPTIMIZATION=true
REACT_APP_CACHE_TTL=300000

# Security
REACT_APP_RATE_LIMIT_ENABLED=true
REACT_APP_CORS_ORIGIN=http://localhost:3000
