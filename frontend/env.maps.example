# Configuration des variables d'environnement pour Expérience Tech

# Google Maps API Key
# Obtenez votre clé API sur: https://console.cloud.google.com/
# Activez les APIs suivantes:
# - Maps JavaScript API
# - Places API
# - Geocoding API
REACT_APP_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# Configuration de l'application
REACT_APP_APP_NAME=Expérience Tech
REACT_APP_APP_VERSION=1.0.0
REACT_APP_APP_DESCRIPTION=Plateforme numérique pour Expérience Tech

# Configuration des langues
REACT_APP_DEFAULT_LANGUAGE=fr
REACT_APP_SUPPORTED_LANGUAGES=fr,en,ar

# Configuration de l'API Backend
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_API_TIMEOUT=10000

# Configuration des cartes
REACT_APP_DEFAULT_MAP_CENTER_LAT=12.1348
REACT_APP_DEFAULT_MAP_CENTER_LNG=15.0557
REACT_APP_DEFAULT_MAP_ZOOM=6

# Configuration des localisations par défaut
REACT_APP_DEFAULT_COUNTRY=Chad
REACT_APP_DEFAULT_CITY=N'Djamena
REACT_APP_DEFAULT_TIMEZONE=Africa/Ndjamena

# Configuration du mode développement
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=info

# Configuration des services externes
REACT_APP_ANALYTICS_ID=
REACT_APP_SENTRY_DSN=

# Configuration des notifications
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_NOTIFICATION_DURATION=5000

# Configuration du cache
REACT_APP_CACHE_DURATION=3600000
REACT_APP_ENABLE_OFFLINE_MODE=false
